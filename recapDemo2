package recapDemo2;

public class Main {

	public static void main(String[] args) {
		
		double[] myList = {1.2, 7.3, 4.3, 5.6};
		double toplam = 0;
		double max = myList[0];
		
		for(double sayi :myList) {
			if (max < sayi) {
				max = sayi;
			}
			toplam = toplam+sayi;
			System.out.println(sayi);
		}
		System.out.println("toplam = " +toplam);
		System.out.println("En Buyuk Sayi: "+max);

	}

}
